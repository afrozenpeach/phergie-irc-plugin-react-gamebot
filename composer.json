{
    "name": "frozen-solid/phergie-irc-plugin-react-onenightrevolution",
    "description": "Phergie plugin for Play One Night Revolution on IRC",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Matt Schraeder",
            "email": "frozen@frozen-solid.net",
            "homepage": "http://frozen-solid.net",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "frozen@frozen-solid.net",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/frozen-solid/phergie-irc-plugin-react-onenightrevolution/issues",
        "source": "https://github.com/frozen-solid/phergie-irc-plugin-react-onenightrevolution"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~2",
        "phergie/phergie-irc-plugin-react-command": "~2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "phake/phake": "2.0.0-beta2"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\One Night Revolution Bot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\One Night Revolution Bot\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
